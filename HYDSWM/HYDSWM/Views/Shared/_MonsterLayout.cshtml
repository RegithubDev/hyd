        <script>
            $("#theme-view").click(function () {
                if (this.checked)
                    sessionStorage.setItem('theme', 'dark')
                else
                    sessionStorage.setItem('theme', 'light')
            });



            $(document).ready(function () {
                $("#portalLogoSmall").hide();
                if (sessionStorage.getItem('theme') == 'dark') {
                    $('#theme-view').attr('checked', true);
                    $('body').attr('data-theme', sessionStorage.getItem('theme'))
                } else {
                    $('#theme-view').attr('checked', false);
                    $('body').attr('data-theme', sessionStorage.getItem('theme'))
                }
                //alert(sessionStorage.getItem('theme'))
            })
            function ShowHide() {
                if ($('#main-wrapper').attr('data-sidebartype') == 'full') {
                    $("#portalLogo").hide();
                    $("#portalLogoSmall").show();
                } else {
                    $("#portalLogoSmall").hide();
                    $("#portalLogo").show();
                }


            }

            window.addEventListener('resize', function () {
                ShowHide();
            })
            
            var timoutWarning = 29 * 6 * 10000;//260000; // Display warning in 14 Mins.
            var timoutNow = 30 * 6 * 10000;//300000; // Timeout in 15 mins.
            var logoutUrl = '/Account/Logout';

            var warningTimer;
            var timeoutTimer;
            var t;

            // Start timers.
            function StartTimers() {
                warningTimer = setTimeout("IdleWarning()", timoutWarning);
                timeoutTimer = setTimeout("IdleTimeout()", timoutNow);

                clearInterval(t);
                Showsecond();
            }
            function Showsecond() {
                var seconds = 0;
                seconds = timoutNow / 1000;
                // document.getElementById("secondsIdle").innerHTML = "";
                t = setInterval(function () {


                    document.getElementById("secondsIdle").innerHTML = seconds;
                    seconds--;
                }, 1000);
            }

            // Reset timers.
            function ResetTimers() {

                
                clearTimeout(warningTimer);
                clearTimeout(timeoutTimer);

                StartTimers();

                //if ($('#timeout').dialog('isOpen') === true)
                // $("#timeout").dialog('close');
                if ($('#timeout').hasClass('show')) {
                    $('#timeout').modal('hide');
                }

            }

            // Show idle timeout warning dialog.
            function IdleWarning() {

                $("#timeout").modal('show');


            }

            //$(function () {
            //    IdleTimeout();
            //});
            // Logout the user.
            function IdleTimeout() {
                localStorage.clear();
               // window.location.href = '/Account/Login';
                $.ajax({
                    type: "Get",
                    contentType: "application/json; charset=utf-8",
                    url: "/Account/LogOffApp",
                    data: "{}",
                    dataType: "json",
                    success: function (data) {
                        //alert(data.d);
                        window.location.href = '/Account/Login';
                        // window.location = "/Account/Logout";//logoutUrl;
                    }
                });


            }
        </script>
