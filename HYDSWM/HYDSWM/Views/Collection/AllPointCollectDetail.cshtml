<div class="content pt-0">

    <!-- Page header -->
    <div class="page-header border-bottom-0">
    </div>
    <!-- /page header -->

    <div class="card mt-1 mr-2">
        <div class="card-header header-elements-inline pt-0 pb-0 ">
            <div class="page-title d-flex mt-4">
                <h4><i class="icon-arrow-left52 mr-2"></i> <span class="font-weight-semibold" id="spnHeader">All Point Collection </span> Report</h4>&nbsp;

            </div>

            <div class="list-icons">
                <a class="list-icons-item" data-action="collapse"></a>
                <a class="list-icons-item" data-action="reload"></a>
                <a class="list-icons-item" data-action="remove"></a>
            </div>
        </div>
        <div class="card-body">


            <div class="row">

                <div class="col-md-3">
                    <label>From Date</label>
                    <input type="text" placeholder="Select From Date" class="form-control" id="txtFromDate" autocomplete="off">
                </div>

                <div class="col-md-3">
                    <label>To Date</label>
                    <input type="text" placeholder="Select To Date" id="txtToDate" class="form-control" autocomplete="off">
                </div>
                <div class="col-md-3" id="dvPointName">
                    <label>Point Name</label>
                    <select id="ddlPointName" data-placeholder="Select Point Name" class="form-control form-control-select2" style="width: 105% ">
                        <option value="0">All  Point Name</option>

                    </select>
                </div>
                <div class="col-md-3">
                    <label>Vehicle Number</label>
                    <select id="ddlVehicleNumber" data-placeholder="Select Vehicle Number" class="form-control form-control-select2" style="width: 105% ">
                        <option value="0">All Vehicle Number</option>

                    </select>
                </div>

                
            </div>

            <div class="row">
                <div class="col-md-3">
                    <label>Zone</label>
                    <select id="ddlZone" onchange="CallFCircleByZone();" data-placeholder="Select Your Zone" class="form-control form-control-select2" style="width: 110% ">
                        <option value="0">All Zone</option>
                    </select>

                    @*<select id="ddlZone" onchange="CallFCircleByZone();" data-placeholder="Select Your Zone" class="form-control form-control-select2" data-fouc>
                    <option value="0">All Zone</option>
                </select>*@

                </div>
                <div class="col-md-3">
                    <label>Circle</label>
                    <select id="ddlCircle" onchange="CallFWardByCircle();" data-placeholder="Select Your Circle" class="form-control form-control-select2" style="width: 110% ">
                        <option value="0">All Circle</option>
                    </select>
                </div>
                <div class="col-md-3">
                    <label>Ward</label>
                    <select id="ddlWard" data-placeholder="Select Your Ward" class="form-control form-control-select2" style="width: 110% ">
                        <option value="0">All Ward</option>
                    </select>

                </div>

                <div class="col-md-3">
                    <label>Route</label>
                    <select id="ddlRoute" onchange="CallAllTripByRoute();" data-placeholder="Select Route" class="form-control form-control-select2" data-fouc style="width: 110% ">
                        <option value="0">All Route</option>
                    </select>

                    @*<select id="ddlRoute" data-placeholder="Select Route" class="form-control form-control-select2" data-fouc>
                    <option value="0">All Route</option>
                </select>*@

                </div>
                <div class="col-md-2">
                    <div class="button-group">
                        <input type="button" id="btnSearch" class="btn btn-primary" value="Search" style="top:20px" onclick="GetDataTableData('Click');">
                        <div style="top:-16px; right:-94px" id="btnExcel" class="btn-group"
                             role="group"
                             aria-label="Button group with nested dropdown">


                            <div class="btn-group" role="group">
                                <button id="btnGroupDrop1"
                                        type="button"
                                        class="btn btn-success  dropdown-toggle"
                                        data-bs-toggle="dropdown"
                                        aria-haspopup="true"
                                        aria-expanded="false" style="top: 34px;right: 90px;">
                                    Export
                                </button>
                                <div class="dropdown-menu"
                                     aria-labelledby="btnGroupDrop1">
                                    <a class="dropdown-item" href="javascript:void(0)" onclick="DownloadFile('Excel');"><strong>Excel</strong></a>
                                    <a class="dropdown-item" href="javascript:void(0)" onclick="CSVData('CSV');"><strong>CSV</strong></a>
                                </div>
                            </div>

                        </div>
                        @*<input type="button" id="btnExcel" class="btn btn-success" style=" left: 83px; top: -16px" value="Export To Excel" onclick="DownloadFile('Excel');">*@
                    </div>
                </div>

            </div>
        </div>

        <div class="dataTables_wrapper no-footer">

            <div class="datatable-scroll-wrap" id="dv1">

                <table class="table datatable-button-html5-columns" id="example">
                    <thead>
                        <tr>
                            <th>Sr. No</th>
                            <th>Vehicle No</th>
                            <th>Route Id</th>
                            <th>Trip</th>
                            <th>Point Name</th>
                            <th>Status</th>
                            <th>Address</th>
                            <th>Collection Date & Time</th>
                            <th>Before Photo</th>
                            <th>After Photo</th>
                            <th>Cordinates</th>
                            <th>Shift Name</th>
                            <th>Shift Date</th>
                        </tr>
                    </thead>
                    <tfoot>
                    </tfoot>
                    <tbody></tbody>
                </table>



            </div>


        </div>
    </div>

</div>

<div id="viewonmap" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title">View On Map</h3>
                <button type="button" class="close" data-dismiss="modal" onclick="CloseModal()">&times;</button>
            </div>
            <div class="modal-body">
                <div id="dvIMap" style="height: 400px; width: 100%">
                </div>
            </div>
            <div class="card-footer d-flex justify-content-between align-items-center bg-teal-400 border-top-0">
                <button type="button" class="btn bg-transparent text-white border-white border-2" data-dismiss="modal" onclick="CloseModal()">Close</button>
            </div>
        </div>
    </div>
</div>

<style>
    td.details-control {
        background: url('/otherfiles/Light/global_assets/images/details_open.png') no-repeat center center;
    }

    tr.shown td.details-control {
        background: url('/otherfiles/Light/global_assets/images/details_close.png') no-repeat center center;
    }
</style>
<script>
    function CloseModal() {
        $(".modal").modal('hide');
    }
</script>

