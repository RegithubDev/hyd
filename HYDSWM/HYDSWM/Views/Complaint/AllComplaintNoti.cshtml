<div class="content pt-0">


    <!-- /page header -->

    <div class="card mt-1 mr-2">
        <div class="card-header header-elements-inline pt-0 pb-0">
            <div class="page-title d-flex mt-4">
                <h4><i class="icon-arrow-left52 mr-2"></i> <span class="font-weight-semibold" id="spnHeader">All Complaint </span> Information</h4>&nbsp;

            </div>

            <div class="list-icons">
                    @*<a title="Download QR Code" cid="" onclick="GetMultipleQrCode(this);" id="btnDownload"><i class="icon-download7"></i></a>*@
                    <a title="Add" cid="" onclick="CallFunc(this,1);" id="btnAdd"><i class="icon-add"></i></a>
                <a class="list-icons-item" data-action="collapse"></a>
                <a class="list-icons-item" data-action="reload"></a>
                <a class="list-icons-item" data-action="remove"></a>
            </div>
        </div>
        <div class="card-body">

        </div>

        <div class="dataTables_wrapper no-footer">

            <div class="datatable-scroll-wrap">
                <div class="col-md-6 row">
                    <div class="button-group">


                        @*<input type="button" id="btnSearch" class="btn btn-primary" value="Search" onclick="GetDataTableData('Click');">*@
                        <input type="button" id="btnExcel" class="btn btn-success" value="Export To Excel" onclick="DownloadFile('Excel');">
                    </div>
                </div>

                <table class="table datatable-button-html5-columns vehicletable" id="example">
                    <thead>
                        <tr>
                            <th>Sr. No</th>
                            <th>Image</th>
                            <th>CG No</th>
                            <th>GCC COMP REF</th>
                            <th>COMPLAINT NAME</th>
                            <th>COMPLAINT CONTACT No</th>
                            <th>COMPLAINT CATEGORY</th>
                            <th>DESCRIPTION</th>
                            <th>ZONE</th>
                            <th>WARD NO</th>
                            <th>STREET LOCATION</th>
                            <th>LOCATION</th>
                            <th>COMPLAINT CREATED ON</th>
                            <th>COMPLAINT CLOSURE ON</th>
                            <th>MODE OF REPORTING</th>
                            <th>PREDEFINED SLA DURATION(MIN)</th>
                            <th>ACTUAL DURATION(HR:MIN)</th>
                            <th>STATUS</th>
                            <th>CLOSE TIME LOCATION</th>
                            <th>CLOSING IMAGE</th>
                            <th>ACTION REMARKS</th>
                            <th>REVISEED WARD NO</th>
                            <th>REVISEED STREET LOCATION</th>
                            <th>ACTION</th>
                            
                            

                        </tr>
                    </thead>
                    <tfoot>
                    </tfoot>
                    <tbody></tbody>
                </table>
                

            </div>
        </div>
    </div>

</div>
<input type="hidden" id="hfRoleId" value="@ViewBag.RoleId" />
<input type="hidden" id="hfLoginid" value="@ViewBag.LoginId" />

<!-- Vertical form modal -->
<div id="modal_form_AddDetail" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <form class="form-validate-jquery" onsubmit="return Formsubmit();">
            <div id="user_content">
            </div>

        </form>
    </div>
</div>
<!-- /vertical form modal -->



<script>
    function CloseModal() {
        $(".modal").modal('hide');
    }
</script>
