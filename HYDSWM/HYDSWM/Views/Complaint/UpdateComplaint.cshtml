                                <option value="4">50</option>
                                <option value="5">59</option>
                                <option value="6">62</option>
                                <option value="7">66</option>
                                <option value="8">76</option>
                                <option value="9">100</option>
                                <option value="10">Depo</option>
                                <option value="11">70</option>
                                <option value="12">17</option>
                                <option value="13">28</option>
                                <option value="14">33</option>
                                <option value="15">34</option>
                                <option value="16">55</option>
                                <option value="17">57</option>
                                <option value="18">69</option>
                                <option value="19">87</option>
                                <option value="20">88</option>
                                <option value="21">95</option>
                                <option value="22">98</option>
                                <option value="23">103</option>
                                <option value="24">Depot</option>
                                <option value="25">11</option>
                                <option value="26">13</option>
                                <option value="27">15</option>
                                <option value="28">40</option>
                                <option value="29">42</option>
                                <option value="30">43</option>
                                <option value="31">51</option>
                                <option value="32">68</option>
                                <option value="33">72</option>
                                <option value="34">77</option>
                                <option value="35">80</option>
                                <option value="36">86</option>
                                <option value="37">92</option>
                                <option value="38">96</option>
                                <option value="39">Depot</option>
                            </select>
                        </div>

                <input type="hidden" id="SComplaintId" value="1" />

                         <div class="col-sm-3">
                           <label>Revised Street Location</label>
                              <span class="text-danger">*</span>
                               <select id="ddlCategory" required="" class="form-control">
                                    <option value="">Select</option>
                        <option value="2">Closed</option>
                        <option value="3">Invalid</option>
                               </select>
                            </div>

                                 <div class="col-sm-3">
                                      <label>Status</label>
                                        @@<span class="text-danger"></span>
                                              <select id="Status"
                                                 class="form-control">
                                          <option value="">Select</option>
                                             <option value="2">Closed</option>
                                               <option value="3">Invalid</option>
                                         </select>
                                       </div>

                         <div class="form-group">
                             <div class="row">

                                       <div class="col-sm-3">
                                         <label>Action Remarks</label>
                                          <input type="hidden" id="hfComplaint" />
                                            <span class="text-danger">*</span>
                                             <input type="text" placeholder="Action Remarks" 
                                             class="form-control" id="Action_Remark" onkeypress="return isAlpha(event.keyCode);" required>
                                        </div>

                        <div class="col-sm-3">
                            <label>Upload File</label>
                            <input type="file" id="files" name="files" class="form-control h-auto" accept=".jpg,.jpeg,.png,.gif,.bmp" value="">


                        </div>

                                      <div class="col-sm-3">
                                          <label>Address</label>
                                           <span class="text-danger">*</span>
                                    <textarea rows="3" cols="3" class="form-control" placeholder="Address" 
                                                         id="address" required=""></textarea>
                                      </div>
                            
                        
         
     <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3806.452697041917!2d78.39076592375736!3d17.43803374982052!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bcb9144cdba8c47%3A0x937fe346f411a645!2sTutorials%20Point%20(India)%20Ltd.!5e0!3m2!1sen!2sin!4v1673629212535!5m2!1sen!2sin"
                            width="600"
                            height="550"
                            style="border:0;"
                            allowfullscreen="" loading="lazy"
                            referrerpolicy="no-referrer-when-downgrade">
                    </iframe>


                </div>
            </div>
        </div>
        <div class="card-footer d-flex justify-content-between align-items-center bg-info border-top-0">
            <button type="button" class="btn bg-transparent text-white border-white border-2" data-dismiss="modal" onclick="CloseModal()">Close</button>
            <button type="submit" class="btn btn-outline bg-white text-white border-white border-2">Submit <i class="icon-paperplane ml-2"></i></button>
        </div>
    </div>
</div>
    <script type="text/javascript">


        function isAlpha(keyCode) {

            return ((keyCode >= 65 && keyCode <= 90) || keyCode == 8 || keyCode == 32 || keyCode == 190 || (keyCode >= 48 && keyCode <= 57) || (keyCode >= 97 && keyCode <= 122))

        }
        $(document).keydown(function (keyCode) {
            if (keyCode.ctrlKey == true && (keyCode.which == '118' || keyCode.which == '86')) {

                keyCode.preventDefault();
            }
        });
    </script>