                    <input type="text" placeholder="Complaint No." class="form-control" id="complaint_num" onkeypress="return isAlpha(event.keyCode);" required>
                </div>
               
                <div class="col-sm-3">
                            <label> Revised Ward No.</label>
                            <span class="text-danger">*</span>
                            <select id="revised_ward_num" required="" class="form-control">
